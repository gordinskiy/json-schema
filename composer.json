{
    "name": "gordinskiy/json-schema",
    "description": "A library to generate JSON Schema",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "json",
        "schema"
    ],
    "authors": [
        {
            "name": "Dmitriy Gordinskiy",
            "email": "dmitriy.gordinskiy94@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.31",
        "justinrainbow/json-schema": "^5.2",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.1",
        "vimeo/psalm": "^5.11",
        "friendsofphp/php-cs-fixer": "^3.34"
    },
    "autoload": {
        "psr-4": {
            "Gordinskiy\\JsonSchema\\": "src/",
            "Gordinskiy\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
